{
  "compilerOptions": {
    "allowJs": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "target": "ES6",
    "declaration": true,
    "outDir": "dist/compiled",
    "removeComments": false,
    "baseUrl": "src",
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules"
  ],
  "typeAcquisition": {
    "enable": true,
    "include": []
  },
}